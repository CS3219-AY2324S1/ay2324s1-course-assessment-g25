module.exports = (sequelize, DataTypes) => {
    const AttemptedQuestions = sequelize.define('Pair', {
        roomId: {
            type: DataTypes.STRING,
            allowNull: false,
        },
        username: {
            type: DataTypes.STRING,
            allowNull: false,
        },
        questionId: {
            type: DataTypes.STRING,
            allowNull: false,
        },
        createdAt: {
            type: DataTypes.DATE,
            allowNull: false,
            defaultValue: DataTypes.NOW,
        },
        attemptedDate: {
            type: DataTypes.DATE,
            allowNull: false,
        }
    }, {
        tableName: 'attemptedQns',
        timestamps: false, // If you don't want Sequelize to manage createdAt and updatedAt columns
    })
    return AttemptedQuestions;
}